
include head
link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous")
script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous")
script(src='https://cdn.jsdelivr.net/npm/dayjs@1.10.4/min/dayjs.min.js', integrity='sha384-3s3qXo+PvCmBtJzBtKcRpG+7bQnQ1tKzHtJfB9Xp+ZtG0Z0jKCuMYcdJ+8OFtdxE', crossorigin='anonymous')

body
  div(style='overflow-x: auto;')
    table.table(style='width: 100%;')
        thead
          tr
            th(scope="col") ID
            th(scope="col") Firs Name
            th(scope="col") Last Name
            th(scope="col") TC Identification number
            th(scope="col") Address
            th(scope="col") Address2
            th(scope="col") Province
            th(scope="col") Town
            th(scope="col") Neighborhood
            th(scope="col") Phone Number
            th(scope="col") E-Mail
            th(scope="col") Birth Day
            th(scope="col") Have You Engaged In Retail Trade?
            th(scope="col") What Is Your Investment Amount?
            th(scope="col") What Is The Reason For Choosing Lokumcu Baba?
            th(scope="col") Want To Add
        tbody
         each forms in form
          tr
            th #{forms.id}
            td #{forms.firstName}
            td #{forms.lastName}
            td #{forms.tc}
            td #{forms.address}
            td #{forms.address2}
            td #{forms.province}
            td #{forms.town}
            td #{forms.neighborhood}
            td #{forms.phoneNumber}
            td #{forms.eMail}
            td #{forms.birthDay}
            td #{forms.occupation}
            td #{forms.amount}
            td #{forms.choosing}
            td #{forms.added}
